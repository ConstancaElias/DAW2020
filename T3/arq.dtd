<?xml version="1.0" encoding="UTF-8"?>

<!ELEMENT ARQUSITS (ARQELEM*)>

<!ELEMENT ARQELEM (TIPO, IDENTI, IMAGEM*, DESCRI, CRONO*, LUGAR, FREQ, CONCEL, CODADM, LATITU, LONGIT, ALTITU, ACESSO, QUADRO, DESARQ, INTERP, DEPOSI, BIBLIO+, AUTOR, TRAARQ*,DATA)>

<!ELEMENT TIPO EMPTY>
